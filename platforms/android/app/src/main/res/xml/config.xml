<?xml version='1.0' encoding='utf-8'?>
<widget id="com.naat.app" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="AdMob">
        <param name="android-package" value="com.cupertino.cordova.plugin.AdMob" />
        <param name="onload" value="true" />
    </feature>
    <name>Naat</name>
    <description>
        Naat collection for Ramadan.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Raza
    </author>
    <content src="index.html" />
    <access origin="*" />
    <slider src="img/slider1.jpg" />
    <icon src="res/icon/android/images.jpg" />
    <splash height="480" src="img/splash.png" width="320" />
    <logo src="img/logo1.png" />
    <colour getcolour="#00008B" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="android-minSdkVersion" value="15" />
</widget>
