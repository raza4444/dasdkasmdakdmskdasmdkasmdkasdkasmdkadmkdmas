<?xml version='1.0' encoding='utf-8'?>
<widget id="com.naat.app" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Naat</name>
    <description>
        Naat collection for Ramadan.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Raza
    </author>
    <content src="index.html" />
    <access origin="*" />
    <slider src="img/slider1.jpg" />
    <icon src="img/images.jpg" />
    <splash height="480" src="img/splash.png" width="320" />
    <logo src="img/logo1.png" />
    <colour getcolour="#00008B" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <preference name="android-minSdkVersion" value="15" />
    <plugin name="onesignal-cordova-plugin" spec="^2.4.1" />
    <plugin name="cordova-admob" spec="^4.1.16" />
</widget>
